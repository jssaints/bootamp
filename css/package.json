{
  "name": "BootAMPSetup",
  "version": "1.0.0",
  "description": "BootAMP build dependencies. Compile to bootamp.css and minifying bootamp.min.css",
  "scripts": {
    "build": "postcss css/bootamp.src.css -o css/bootamp.css --config postcss.config.js",
    "minify": "postcss css/bootamp.css -u cssnano -o css/bootamp.min.css --no-map"
  },
  "author": "JSSaints.com",
  "license": "",
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "cssstats": "^4.0.0",
    "postcss": "^8.2.1",
    "postcss-calc": "^7.0.5",
    "postcss-cli": "^8.3.1",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^10.0.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^14.0.0",
    "postcss-importantly": "0.0.7",
    "postcss-loader": "^4.1.0",
    "postcss-remove-root": "0.0.2",
    "postcss-reporter": "^7.0.2"
  }
}
